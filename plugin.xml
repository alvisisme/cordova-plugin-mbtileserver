<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.plugin.mbtileserver" version="0.1.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>MbtileServer</name>
    <js-module name="MbtileServer" src="www/MbtileServer.js">
        <clobbers target="cordova.plugins.MbtileServer" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="MbtileServer">
                <param name="android-package" value="com.plugin.mbtileserver.MbtileServer" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/MbtileServer.java" target-dir="src/com/plugin/mbtileserver/MbtileServer" />
    </platform>
</plugin>